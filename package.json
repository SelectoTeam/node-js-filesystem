{
  "name": "js-filesystem",
  "version": "0.0.1",
  "description": "Filesystem abstraction: Many filesystems, one API.",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"+(src|test)/**/*.ts\"",
    "test": "mocha --require ts-node/register test/*.ts test/**/*.ts",
    "report-coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lywzx/node-js-filesystem.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "filesystem"
  ],
  "author": "liuyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lywzx/node-js-filesystem/issues"
  },
  "homepage": "https://github.com/lywzx/node-js-filesystem#readme",
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^8.1.0",
    "@types/lodash": "^4.14.150",
    "@types/mime": "^2.0.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.13.2",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.2",
    "mocha": "^7.0.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.2"
  },
  "dependencies": {
    "debug": "^4.1.1",
    "file-type": "^14.2.0",
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.15",
    "mime": "^2.4.5"
  }
}
